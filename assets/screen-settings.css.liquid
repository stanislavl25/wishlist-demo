:root {
  --secondary_text:     var(--white);
  --secondary_bg:       {{ settings.palette_accent }};
  --secondary_bg_dark:  {{ settings.palette_accent | color_darken: 10 }};
  --secondary_bg_fade:  {{ settings.palette_accent | color_lighten: 10 }};
  --secondary_fg:       var(--secondary_text);

  --secondary_btn_text:    {{ settings.regular_button_text_color }};
  --secondary_bg_btn:      {{ settings.regular_button_color }};
  --secondary_bg_btn_dark: {{ settings.regular_button_color | color_darken: 10 }};
  --secondary_bg_btn_fade: {{ settings.regular_button_color | color_lighten: 10 }};

  --tertiary_text:      {{ settings.buy_button_text_color }};
  --tertiary_bg:        {{ settings.buy_button_color }};
  --tertiary_bg_dark:   {{ settings.buy_button_color | color_darken: 10 }};
  --tertiary_bg_fade:   {{ settings.buy_button_color | color_lighten: 10 }};
  --tertiary_fg:        var(--tertiary_text);

  --quaternary_text:    {{ settings.dynamic_buy_button_text_color }};
  --quaternary_bg:      {{ settings.dynamic_buy_button_color }};
  --quaternary_bg_dark: {{ settings.dynamic_buy_button_color | color_darken: 10 }};
  --quaternary_bg_fade: {{ settings.dynamic_buy_button_color | color_lighten: 10 }};
  --quaternary_fg:      var(--quaternary_text);

  --custom_top_search_bg:var(--body_bg);
  --custom_input_fg:    {{ settings.palette_black }};
  --custom_input_bd: {{ settings.palette_black | color_to_rgb | color_modify: 'alpha', 0.08 }};

  --alert_error:        {{ settings.negative_vibes }};
  --alert_valid:        {{ settings.positive_vibes }};
  --lime:               {{ settings.positive_vibes }};
  --gallery:            {{ settings.palette_light }};
  --sand:               {{ settings.palette_light }};

  --product_label_bg:   {{ settings.product_label_color }};
  --product_label_bg_dark:{{ settings.product_label_color | color_darken: 5 }};
  --product_label_text: {{ settings.product_label_text_color }};
  --sale_label_bg:      {{ settings.sale_label_color }};
  --sale_label_bg_dark: {{ settings.sale_label_color | color_darken: 5 }};
  --sale_label_text:    {{ settings.sale_label_text_color }};
  --product_label_bg_custom: {{ settings.preorder_label_color }};
  --product_label_text_custom: {{ settings.preorder_label_text_color }};


  --positive_bg:        {{ settings.positive_vibes }};
  --positive_bg_dark:   {{ settings.positive_vibes | color_darken: 5 }};
  --positive_fg:	      var(--white);

  --price_color:        {{ settings.price_color }};
  --price_color_old:    {{ settings.compare_at_price_color }};

  --primary_text:       var(--{{ settings.text_color }});
  --primary_text_h:     var(--{{ settings.title_color }});
{% liquid
    assign text_color_handle = 'palette_' | append: settings.text_color
    assign text_color = settings[text_color_handle]
%}
  --primary_text_rgba:  {{ text_color | color_to_rgb }};

  --body_bg:            {{ settings.body_background_color }};

  --breadcrumbs_fz:  {{ settings.breadcrumbs_font_size }};
  --main_ff_h:          {% if custom_primary_font %}'{{ settings.custom_primary_font_name }}', {% endif %}{{ settings.primary_font.family | remove: '"' }}, {{ settings.primary_font.fallback_families }};
  --main_fw_h:          {{ settings.heading_font_weight }};
  --main_fw_h_strong:   {{ settings.heading_font_weight | plus: 300 | at_most: 700 }};
  --main_fw_bold:       var(--main_fw_strong);
  --main_fs_h:          {{ settings.primary_font.style }};
  --main_ls_h:          {% if settings.primary_letter_spacing == '0' %}normal{% else %}{{ settings.primary_letter_spacing | divided_by: 100.0 }}em{% endif %};
  --main_tt_h:          {{ settings.primary_case }};
  --main_lh_h:          {{ settings.heading_line_height | divided_by: 100.0 }};

{% if settings.body_font == 'primary' %}
  --main_ff:            var(--main_ff_h);
  --main_fs:            var(--main_fs_h);
{% else %}
  --main_ff:            {% if custom_secondary_font %}'{{ settings.custom_secondary_font_name }}', {% endif %}{{ settings.secondary_font.family | remove: '"' }}, {{ settings.secondary_font.fallback_families }};
  --main_fs:            {{ settings.secondary_font.style }};
{% endif %}
  --main_tt:            {{ settings.body_case }};
  --main_fw:            {{ settings.body_font_weight }};
  --main_fw_strong:     {{ settings.body_font_weight | plus: 300 | at_most: 700 }};

  --main_ls:            {% if settings.body_letter_spacing == '0' %}normal{% else %}{{ settings.body_letter_spacing | divided_by: 100.0 }}em{% endif %};
  --main_lh:            {{ settings.body_line_height | divided_by: 100.0 }};
  --main_fz:            {{ settings.body_font_size }};

  --main_h1:            {{ settings.h1_size }}px;
  --main_h2:            {{ settings.h2_size }}px;
  --main_h3:            {{ settings.h3_size }}px;
  --main_h4:            {{ settings.h4_size }}px;
  --main_h_small:       {{ settings.h5_size }}px;

  --mob_h1:             {{ settings.h1_size_mobile }}px;
  --mob_h2:             {{ settings.h2_size_mobile }}px;
  --mob_h3:             {{ settings.h3_size_mobile }}px;
  --mob_h4:             {{ settings.h4_size_mobile }}px;
  --mob_h5:             {{ settings.h5_size_mobile }}px;
  --mob_h_small:        {{ settings.h5_size_mobile }}px;
  --mob_fz:             {{ settings.body_font_size_mobile }};
  --btn_fz_mob:         {{ settings.button_fontsize_mobile }};

{% if settings.button_font == 'primary' %}
  --btn_ff:            var(--main_ff_h);
  --btn_fs:            var(--main_fs_h);
{% else %}
  --btn_ff:            {% if custom_secondary_font %}'{{ settings.custom_secondary_font_name }}', {% endif %}{{ settings.secondary_font.family | remove: '"' }}, {{ settings.secondary_font.fallback_families }};
  --btn_fs:            {{ settings.secondary_font.style }};
{% endif %}
  --btn_fw:            {{ settings.button_font_weight }};
  --btn_fz:          {{ settings.button_fontsize }};
  --btn_tt:          {{ settings.button_case }};
  --btn_ls:          {% if settings.button_letter_spacing == '0' %}normal{% else %}{{ settings.button_letter_spacing | divided_by: 100.0 }}em{% endif %};
  --btn_bd: {{ settings.button_border }}px;
  --btn_br: {{ settings.button_radius }}px;
  --btn_pv: {{ settings.button_padding_vertical }}px;
  --btn_ph: {{ settings.button_padding_horizontal }}px;


{% if settings.prices_font == 'primary' %}
  --price_ff:            var(--main_ff_h);
  --price_fs:            var(--main_fs_h);
{% else %}
  --price_ff:            {% if custom_secondary_font %}'{{ settings.custom_secondary_font_name }}', {% endif %}{{ settings.secondary_font.family | remove: '"' }}, {{ settings.secondary_font.fallback_families }};
  --price_fs:            {{ settings.secondary_font.style }};
{% endif %}
  --price_fw:            {{ settings.prices_font_weight }};
  --price_fz:            {{ settings.product_prices_fontsize }}px;

{% unless settings.everything_rounded %}
  --b2r: 0;
{% endunless %}
  --b2i: {{ settings.input_border_radius }}px;
  --b2p: {{ settings.images_border_radius }}px;

  --glw: {% if settings.width == 2000 %}10000{% else %}{{ settings.width }}{% endif %}px;
  --ratio: {{ settings.custom_product_image_ratio | divided_by: 100.0 }};
  --btn_dist: 10px;
}
.l4pr { --ratio: {{ settings.custom_product_image_ratio | divided_by: 100.0 }}; }

{% if settings.width > 1600 %}
body.template-product {
  --glw: 1600px;
}
{% endif %}
@media only screen and (max-width: 760px) {
  :root {
    --main_mr: 20px;
    --size_70_f: 12vw;
    --main_fz: var(--mob_fz);
    --btn_fz: var(--btn_fz_mob);
    --main_h_small: var(--size_13_f);
    --main_h1: var(--mob_h1);
    --main_h2: var(--mob_h2);
    --main_h3: var(--mob_h3);
    --main_h4: var(--mob_h4);
    --main_h5: var(--mob_h5);
    --main_h6: var(--mob_h6);
    --main_lead: var(--mob_lead);
  {% if settings.button_wide_mobile %}
    --btn_ph: calc(18px);
  {% endif %}
    --b2p: {{ settings.images_border_radius_mobile }}px;
  }
}

