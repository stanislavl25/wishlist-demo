{% comment %}theme-check-disable ImgLazyLoading{% endcomment %}
{%- liquid
  assign search_compact = false
  assign logo_centered = false
  assign search_centered = false
  assign search_right = false
  assign menu_bar = true

  assign app_blocks = section.blocks | where: 'type', '@app'
  assign liquid_blocks = section.blocks | where: 'type', 'liquid'
  assign usp_blocks = section.blocks | where: 'type', 'usp'

  case section.settings.layout
    when 'layout_1'
      assign search_centered = true
      assign elements_order = 'button,logo,searchbar' | split: ','
    when 'layout_2'
      assign elements_order = 'button,logo,searchbar' | split: ','
    when 'layout_3'
      assign search_right = true
      assign search_compact = true
      assign elements_order = 'button,logo,searchbar' | split: ','
    when 'layout_4'
      assign search_right = true
      assign elements_order = 'button,logo,searchbar' | split: ','
    when 'layout_5'
      assign menu_bar = false
      assign search_compact = true
      assign search_right = true
      assign elements_order = 'button,searchbar,logo,menu' | split: ','
    when 'layout_6'
      assign logo_centered = true
      assign search_compact = true
      assign search_centered = true
      assign elements_order = 'button,searchbar,logo' | split: ','
    when 'layout_7'
      if section.settings.logo
        assign logo_centered = true
      elsif shop.name.size <= 45
        assign logo_centered = true
      else
        assign logo_text_centered = true
      endif
      assign elements_order = 'button,searchbar,logo' | split: ','
    when 'layout_8'
      assign menu_bar = false
      assign logo_centered = true
      assign search_compact = true
      assign search_right = true
      assign elements_order = 'button,menu,logo,searchbar' | split: ','
  endcase

  if section.settings.menubar_menu != empty
    assign menu = section.settings.menubar_menu
    assign menu_layout = section.settings.menu_layout
  else
    assign menu = false
    assign menu_layout = false
  endif

  if menu_bar
    assign menu_bar_height = section.settings.menubar_height | remove: 'px'
  endif
  assign header_min_height = section.settings.header_height | split: '|' | first
  assign header_min_height_mobile = section.settings.header_height | split: '|' | last
  assign logo_height = section.settings.logo_width | divided_by: settings.logo.aspect_ratio
  assign logo_height_mobile = section.settings.logo_width_mobile | divided_by: settings.mobile_logo.aspect_ratio
  if section.settings.search_compact_mobile
    assign search_height_mobile = 0
  else
    assign search_height_mobile = section.settings.searchbar_height_mobile | plus: 12
    if section.settings.search_mobile_bd contains 'bd-t'
      assign search_height_mobile = search_height_mobile | plus: 12
    endif
  endif
  assign main_header_height = logo_height | plus: 24 | at_least: header_min_height
  assign predicted_header_height = main_header_height | plus: menu_bar_height
  assign predicted_header_height_mobile = logo_height_mobile | plus: 24 | at_least: header_min_height_mobile | plus: search_height_mobile
-%}

<script>
  {%- if section.settings.sticky == 'sticky-menu' and section.settings.enable_sticky_header -%}
    document.documentElement.classList.add('t1sn');
  {%- endif -%}
  {%- if search_compact -%}
    document.documentElement.classList.add('t1sr');
  {%- endif -%}
    document.documentElement.classList.add('t1nb');
  {%- if menu == false -%}
    document.documentElement.classList.add('t1mn');
    document.documentElement.classList.add('t1nn');
  {%- endif -%}
  {%- if search_centered -%}
    document.documentElement.classList.add('search-compact-is-centered');
  {%- endif -%}
</script>

{%- style -%}
  :root {
  {% comment %} header width {% endcomment %}
  --ghw:  {% if section.settings.width == 2000 %}10000{% else %}{{ section.settings.width }}{% endif %}px;
  {% comment %} top bar {% endcomment %}
  --custom_top_up_bg:           var(--{{ section.settings.top_bar_color }}_bg);
  --custom_top_up_fg:           var(--{{ section.settings.top_bar_color | remove: '_gradient' }}_fg);
  --custom_top_up_fg_hover:     var(--{{ section.settings.top_bar_color | remove: '_gradient' }}_btn_bg);
  --custom_top_up_h: 	 	{{ section.settings.top_bar_height | split: '|' | first }};
  {% comment %} main {% endcomment %}
  --header_mih:                 {{ section.settings.header_height | split: '|' | first }};
  --custom_top_main_bg:         var(--{{ section.settings.header_color }}_bg);
  --custom_top_search_bg_cont:  var(--custom_top_main_bg);
  --custom_top_main_fg:         var(--{{ section.settings.header_color | remove: '_gradient' }}_fg);
  --custom_top_main_link_bg:    var(--{{ section.settings.button_color_palette }}_bg);
  --custom_top_main_link_dark:  var(--{{ section.settings.button_color_palette }}_bg_dark);
  --custom_top_main_link_text:  var(--{{ section.settings.button_color_palette }}_fg);
  {% comment %} menubar {% endcomment %}
  --custom_top_nav_h:           {{ section.settings.menubar_height }};
  --custom_top_nav_bg:          var(--{{ section.settings.menubar_color }}_bg);
  --custom_top_nav_fz:          {{ section.settings.menu_font_size }}px;
  --mobile_nav_fz:              {{ section.settings.menu_font_size_mobile }}px;
  --custom_top_nav_ls:          {% if section.settings.menu_letter_spacing == '0' %}normal{% else %}{{ section.settings.menu_letter_spacing | divided_by: 100.0 }}em{% endif %};
  --custom_top_nav_fw:          {{ section.settings.menu_font_weight }};
  {% if section.settings.enable_menu_subs_font_size %}
    --custom_top_nav_fz_sub:     {{ section.settings.menu_subs_font_size }}px;
  {% endif %}
  {% unless section.settings.header_shadow %}
    --custom_top_main_sh: none;
  {% else %}
    --custom_top_main_sh: 0 9px 32px rgba(0,0,0,{{ section.settings.header_shadow_opacity | divided_by: 100.0}});
  {% endunless %}
  {% if section.settings.header_border_opacity != 0 %}
    {%- liquid
      assign header_border_opacity = section.settings.header_border_opacity | divided_by: 100.0
      assign header_border_color_light = settings.palette_black | color_to_rgb | color_modify: 'alpha', header_border_opacity
      assign header_border_color_dark = settings.palette_white | color_to_rgb | color_modify: 'alpha', header_border_opacity
      if section.settings.menubar_color == 'white' or section.settings.menubar_color == 'light' or section.settings.menubar_color == 'light_2' or section.settings.menubar_color == 'light_gradient'
        assign header_border_color = header_border_color_light
      else
        assign header_border_color = header_border_color_dark
      endif
    -%}
    --custom_top_nav_bd_op:     1;
    --transparent_header_bd_op:   {{ header_border_opacity }};
    --header_border_color_light:  {{ header_border_color_light }};
    --header_border_color_dark:   {{ header_border_color_dark }};
    --custom_top_nav_bd:          {{ header_border_color }};
  {% else %}
    --custom_top_nav_bd_op:     0;
    --transparent_header_bd_op: 0;
    --custom_top_nav_bd:        transparent;
  {% endif %}
  --custom_top_nav_fg:          var(--{{ section.settings.menubar_color | remove: '_gradient' }}_fg);
  --custom_top_nav_fg_hover:    var(--custom_top_nav_fg);
  {% comment %} dropdown {% endcomment %}
  --custom_drop_nav_bg:         var(--{{ settings.dropdown_color }}_bg);
  --custom_drop_nav_fg:         var(--{{ settings.dropdown_color }}_fg);
  --custom_drop_nav_fg_text:    var(--{{ settings.dropdown_color }}_fg);
  --custom_drop_nav_fg_hover:   var(--{{ settings.dropdown_color }}_btn_bg);
  --custom_drop_nav_head_bg:    var(--{{ settings.dropdown_color }}_bg);
  --custom_drop_nav_head_fg:    var(--{{ settings.dropdown_color }}_fg);
  --custom_drop_nav_bd:         var(--{{ settings.dropdown_color }}_bd);
  {% comment %} searchbar {% endcomment %}
  {%- if section.settings.show_searchbar_border -%}
    --custom_top_search_bd:     var(--{{ section.settings.header_color | remove: '_gradient' }}_bd);
  {%- else -%}
    --custom_top_search_bd:     transparent;
  {%- endif -%}
  --custom_top_search_bg:       var(--{{ section.settings.search_color }}_bg);
  --custom_top_search_fg:       var(--{{ section.settings.search_color | remove: '_gradient' }}_fg);
  --custom_top_search_pl:       var(--{{ section.settings.search_color | remove: '_gradient' }}_pl);
  {% comment %} predicted header heights for transparent header {% endcomment %}
  --header_height_static: {{ predicted_header_height }}px;
  @media only screen and (max-width: 760px) {
  --custom_top_search_h:        {{ section.settings.searchbar_height_mobile }}px;
  }
  @media only screen and (min-width: 761px) {
  --custom_top_search_h:        clamp(calc(var(--main_fz) + 20px), {{ section.settings.searchbar_height }}px, calc(var(--header_mih) - 15px));
  --search_w:               {% if section.settings.searchbar_width == 600 %}100%{% else %}{{ section.settings.searchbar_width }}px{% endif %};
    #nav-bar > ul > li > ul > li > ul > li > a, #nav > ul > li > ul > li > ul > li > a { font-weight: {{ section.settings.menu_subs_font_weight }}; }
  }
  }
  @media only screen and (max-width: 62.5em ) {
  #root #header .f8vl *, #root .m6pn.f8vl * {
  --secondary_btn_text:             var(--{{ settings.dropdown_color }}_btn_{% if settings.button_style == 'inv' %}bg{% else %}fg{% endif %});
  --secondary_bg_btn:               var(--{{ settings.dropdown_color }}_btn_bg);
  --secondary_bg_btn_dark:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
  --secondary_bg_btn_fade:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
  --white:                      var(--{{ settings.dropdown_color }}_btn_fg);
  }
  :root {
  --header_mih:                 {{ section.settings.header_height | split: '|' | last }};
  --custom_top_up_h:       {{ section.settings.top_bar_height | split: '|' | last }};
  {% comment %} predicted header heights for transparent header {% endcomment %}
  --header_height_static: {{ predicted_header_height_mobile }}px;
  }
  #root .shopify-section-header #header-inner > .link-btn a:first-child:after {
    content: "\{{ section.settings.menu_icon | split: '|' | last }}";
  }
  }
  #root #header .f8vl .submit, #root .m6pn.f8vl .submit {
  --secondary_btn_text:             var(--{{ settings.dropdown_color }}_btn_fg);
  --secondary_bg_btn:               var(--{{ settings.dropdown_color }}_btn_bg);
  --secondary_bg_btn_dark:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
  --secondary_bg_btn_fade:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
  --secondary_bg_fade:          var(--secondary_bg_btn_fade);
  }
  {% if section.settings.show_header_border == 'top' %}
    #nav.plain:before, #nav-bar.plain:before, .has-first-m6fr-wide .shopify-section-header.transparent:not(.fixed) #nav.tr_bd:before, .has-first-m6fr-wide .shopify-section-header.transparent:not(.fixed) #nav-bar.tr_bd:before { border-bottom-width: 0; }
  {% elsif section.settings.show_header_border == 'bottom' %}
    #nav.plain:before, #nav-bar.plain:before, .has-first-m6fr-wide .shopify-section-header.transparent:not(.fixed) #nav.tr_bd:before, .has-first-m6fr-wide .shopify-section-header.transparent:not(.fixed) #nav-bar.tr_bd:before { border-top-width: 0; }
  {% endif %}
  {% comment %} cart icon {% endcomment %}
  #root .icon-cart:before { content: "\{{ section.settings.cart_icon | split: '|' | last }}"; }
  #nav-user li:not(.link-btn) {
  --custom_top_main_link_bg:    var(--{{ section.settings.cart_icon_color }}_bg);
  --custom_top_main_link_dark:  var(--{{ section.settings.cart_icon_color }}_bg_dark);
  --custom_top_main_link_text:  var(--{{ section.settings.cart_icon_color | remove: '_gradient' }}_fg);
  }
  #shopify-section-{{ section.id }} #nav {
  --custom_drop_nav_head_bg:    var(--{{ settings.dropdown_color }}_bg_var);
  }
  {% if section.settings.menu_enable_custom_text_styling %}
    #nav-bar, #nav { --custom_top_main_fg: var(--{{ section.settings.menu_items_color_palette }}); --custom_top_nav_fg: var(--custom_top_main_fg); }
  {% endif %}
{%- endstyle -%}

{%- capture logo -%}
  <p id="logo" class="{% if section.settings.layout_mobile == 'logo_right' %}mobile-text-end{% endif %}">
    <a href="{{ routes.root_url }}">
      {%- if settings.logo -%}
        {%- capture sizes %}(min-width: 760px) {{ section.settings.logo_width }}px, {{ section.settings.logo_width_mobile }}px{%- endcapture -%}
        {%- capture widths %}{{ section.settings.logo_width }},{{ section.settings.logo_width | times: 2 }}{%- endcapture -%}
        {%- capture widths_mobile %}{{ section.settings.logo_width_mobile }},{{ section.settings.logo_width_mobile | times: 2 }}{%- endcapture -%}
        {%- liquid
          assign width = section.settings.logo_width | times: 2
          assign alt = section.settings.logo.alt | default: shop.name | escape
        -%}

        {%- if settings.darkmode_logo -%}
          <picture class="dark-only">
            {%- if settings.mobile_darkmode_logo -%}
              <source
                media="(max-width: 760px)"
                srcset="
                  {%- assign widths_mobile_arr = widths_mobile | split: ',' -%}
                  {%- for width in widths_mobile_arr -%}
                    {{ settings.mobile_darkmode_logo | image_url: width: width }} {{ width }}w,
                  {%- endfor -%}
                ">
            {%- endif -%}
            {{ settings.darkmode_logo | image_url: width: width | image_tag:
              class: 'logo-img',
              height: logo_height,
              width: section.settings.logo_width,
              widths: widths,
              alt: alt,
              sizes: sizes
            }}
          </picture>
        {%- endif -%}
        <picture {% if settings.darkmode_logo %}class="dark-hide"{% endif %}>
          {%- if settings.mobile_logo -%}
            <source
              media="(max-width: 760px)"
              srcset="
                {%- assign widths_mobile_arr = widths_mobile | split: ',' -%}
                {%- for width in widths_mobile_arr -%}
                  {{ settings.mobile_logo | image_url: width: width }} {{ width }}w,
                {%- endfor -%}
              ">
          {%- endif -%}
          {{ settings.logo | image_url: width: width | image_tag:
            class: 'logo-img',
            height: logo_height,
            width: section.settings.logo_width,
            widths: widths,
            alt: alt,
            sizes: sizes
          }}
        </picture>
      {%- else -%}
        <span>{{ shop.name }}</span>
      {%- endif -%}
    </a>
    {%- if settings.logo -%}
      {% style %}
        #logo {
          --logo_w: {{ section.settings.logo_width }}px;
        }
        @media only screen and (max-width: 47.5em) {
          #logo {
            --logo_w: {{ section.settings.logo_width_mobile }}px;
          }
        }
      {% endstyle %}
    {%- endif -%}
  </p>
{%- endcapture -%}

{%- capture button -%}
  <p class="link-btn">
    <a class="text-justify{% if section.settings.menubar_caps %} mobile-text-uppercase{% endif %}" href="{{ routes.root_url }}" aria-controls="nav">{{ 'header.navigation.view_catalog' | t }} <i aria-hidden="true" class="icon-chevron-down"></i></a>
    <a class="search-compact{% if search_right %} text-end{% endif %}" href="#search" aria-label="search" aria-controls="search"><i aria-hidden="true" class="icon-zoom"></i> <span class="hidden">{{ 'search.title' | t }}</span></a>
  </p>
{%- endcapture -%}

{%- capture searchbar -%}
  <form action="{{ routes.search_url }}" method="get" id="search" class="
    {% if search_compact %}compact{% endif %}
    {% if search_centered %}text-center-sticky{% endif %}
    {% if search_right %}text-end{% endif %}
    {% if search_compact %}wide text-center-sticky{% endif %}
    {% unless settings.enable_search_drawer %}no-autocomplete{% endunless %}
    blur
    {% if section.settings.search_compact_mobile %}m-pos-up{% else %}no-bg {% unless section.settings.search_mobile_bd == 'none' %}{{ section.settings.search_mobile_bd }}{% unless section.settings.search_mobile_bd contains 'bd-t' %} no-pd-t{% endunless %}{% endunless %}{% endif %}
    {% if search_right and search_compact %}compact-handle-mobile{% else %}compact-handle{% endif %}
    "
    style="--main_fz:{{ section.settings.searchbar_font_size }};--price_fz:{{ section.settings.searchbar_font_size }};font-size:{{ section.settings.searchbar_font_size }};"
  >
    <fieldset>
      <legend>{{ 'search.title' | t }}</legend>
      <p>
        <label for="search_main">{{ 'search.title' | t }}</label>
        <input type="search" id="search_main" name="q" placeholder="{{ 'search.search_form.placeholder' | t }}" autocomplete="off" required>
        <button type="submit" class="override">{{ 'search.search_form.submit' | t }}</button>
      </p>
      <div id="livesearch">

        {%- if settings.search_drawer_popular_collections.count > 0 or settings.search_drawer_popular_products.count > 0 -%}
          {%- liquid
            assign show_price = settings.search_drawer_show_price
            if settings.login_for_prices and customer == nil
              assign show_price = false
            endif
          -%}
          <div class="search-placeholders">
            {%- if settings.search_drawer_popular_collections.count > 0 -%}
              {%- if settings.search_drawer_popular_collections_title != empty -%}<p class="strong">{{ settings.search_drawer_popular_collections_title }}</p>{%- endif -%}
              <ul class="l4pl">
                {%- for collection in settings.search_drawer_popular_collections -%}
                  <li><a href="{{ collection.url }}">{{ collection.title }}</a></li>
                {%- endfor -%}
              </ul>
            {%- endif -%}
            {%- if settings.search_drawer_popular_products.count > 0 -%}
              {%- if settings.search_drawer_popular_products_title != empty -%}<p class="strong">{{ settings.search_drawer_popular_products_title }}</p>{%- endif -%}
              <ul class="l4ca compact">
                {%- for product in settings.search_drawer_popular_products -%}
                  <li class="{% if product.featured_media == blank %} no-img{% endif %}{% if settings.fill_product_images %} cover{% endif %}{% if product.compare_at_price > product.price %} has-discount{% endif %}">
                    {%- if product.featured_media != blank -%}
                      <figure{% if settings.multiply_product_images == 'multiply' %} class="img-multiply"{% elsif settings.multiply_product_images == 'multiply-bg' %} class="img-multiply-bg"{% endif %}>
                        <picture>
                          <img src="{{ product.featured_media | image_url: width: 140 }}"
                               srcset="{{ product.featured_media | image_url: width: 70 }} 1x,{{ product.featured_media | image_url: width: 140 }} 2x"
                               alt="{{ product.featured_media.alt }}"
                               width="70"
                               height="71"
                          >
                        </picture>
                      </figure>
                    {%- endif -%}
                    <section>
                      <div class="cols align-middle">
                        <div>
                          <h2>
                            {%- if settings.search_drawer_show_vendor and product.vendor != "vendor-unknown" and product.vendor != shop.name -%}
                              <span class="small">{{ product.vendor }}</span>
                            {%- endif -%}
                            <a href="{{ product.url }}">{{ product.title }} </a>
                          </h2>
                        </div>
                        {%- if show_price -%}
                          <p class="price">
                            {%- if product.compare_at_price > product.price -%}<span class="old-price">{{ product.compare_at_price | money }}</span>{%- endif -%}
                            {{ product.price | money }}
                          </p>
                        {%- endif -%}
                      </div>
                    </section>
                  </li>
                {%- endfor -%}
              </ul>
            {%- endif -%}
          </div>
        {%- else -%}
          <div class="cols">
            <p>{{ 'search.results' | t }}</p>
          </div>
        {%- endif -%}

      </div>
    </fieldset>
  </form>
{%- endcapture -%}

{%- if section.settings.show_top_bar -%}
  <nav id="nav-top" class="ff-{{ section.settings.top_bar_font }}" style="--main_fz:{{ section.settings.top_bar_font_size }}">
    {%- if usp_blocks -%}
      <ul class="l4us mobile-text-center{% if usp_blocks.size > 1 %}{% if section.settings.enable_usp_slider %} slider slider-single{% endif %}{% unless section.settings.enable_usp_slider_arrows %} no-arrows{% endunless %}{% endif %}" {% if section.settings.enable_usp_slider_autoplay %}data-autoplay={{ section.settings.usp_slider_autoplay_seconds | times: 1000 }}{% endif %}>
        {%- for block in usp_blocks limit: 3 -%}
          {%- if block.settings.usp != empty -%}
            <li{% if block.settings.usp_item_icon != 'default' or block.settings.image %} class="no-checks"{% endif %} {{ block.shopify_attributes }}>
              {% if block.settings.image or block.settings.usp_item_icon != 'none' %}
                <div class="cols cols-mobile align-middle" {% if block.settings.usp_item_icon != 'none' %}{% unless block.settings.image %}style="--cols: 2px;"{% endunless %}{% endif %}>
                  {% if block.settings.image %}
                    {%- assign image_width = block.settings.image_width -%}
                    {%- assign image_width_2 = block.settings.image_width | times: 2 -%}
                    <figure class="m0 no-border" style="width: calc({{ image_width }}px + var(--cols));">
                      <picture>
                        <img
                          src="{{ block.settings.image | image_url: height: image_width }}"
                          srcset="{{ block.settings.image | image_url: height: image_width }} 1x,{{ block.settings.image | image_url: height: image_width_2 }} 2x"
                          height="50"
                          width="{{ image_width }}"
                          style="width:{{ image_width }}px!important; height: auto!important;"
                          alt="{{ block.settings.image.alt | default: block.settings.title | escape }}"
                          loading="{% if section.index > 1 or forloop.first == false or section.location == 'footer' %}lazy{% else %}eager{% endif %}"
                        >
                      </picture>
                    </figure>
                  {% else %}
                    {%- render 'icons', icon: block.settings.usp_item_icon -%}
                  {% endif %}
              {% endif %}
              <span>{{ block.settings.usp | replace: '</p><p>', ' ' | remove: '<p>' | remove: '</p>' }}</span>
              {% if block.settings.image or block.settings.usp_item_icon != 'none' %}
                </div>
              {% endif %}
            </li>
          {%- endif -%}
        {%- endfor -%}
      </ul>
    {%- endif -%}
    <ul data-type="top-nav">
      {%- for block in liquid_blocks -%}
        {% if block.settings.liquid != empty and block.settings.location == 'top' %}
          <li{% unless block.settings.show_mobile %} class="desktop-only"{% endunless %}>
            {{ block.settings.liquid }}
          </li>
        {% endif %}
      {%- endfor -%}
      {% if section.settings.navbar_menu_item_text %}
        <li>
          <a href="{{ section.settings.navbar_menu_item_link }}"{% unless section.settings.navbar_menu_item_link %} role="none" style="pointer-events: none"{% endunless %}>
            {% if section.settings.navbar_menu_item_icon != 'none' %}
              {%- render 'icons', icon: section.settings.navbar_menu_item_icon -%}
            {% endif %}
            <span>{{ section.settings.navbar_menu_item_text }}</span>
          </a>
        </li>
      {% endif %}
      {%- liquid
        if section.settings.enable_country_selector and localization.available_countries.size > 1
          render 'language-country-selector', display: 'country', extra_classes: 'mobile-nav-only'
        endif
        if section.settings.enable_language_selector and localization.available_languages.size > 1
          render 'language-country-selector', display: 'language', extra_classes: 'mobile-nav-only'
        endif
      -%}
    </ul>
  </nav>
{%- elsif menu_bar == false %}
  <nav id="nav-top" class="hidden ff-{{ section.settings.menu_font }}" aria-label="{{ 'general.accessibility.menu' | t }}">
    {%- for block in liquid_blocks -%}
      {% if block.settings.liquid != empty and block.settings.location == 'top' %}
        <li class="mobile-nav-only">
          {{ block.settings.liquid }}
        </li>
      {% endif %}
    {%- endfor -%}
    <ul data-type="top-nav">
     {% if section.settings.navbar_menu_item_text %}
        <li>
